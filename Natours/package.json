{
  "name": "natours",
  "version": "1.0.0",
  "description": "\"Including SASS\"",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss -use autoprefixer -b 'last q0 versions' css/style.concat.css -o css/style.prefix.css"
  },
  "author": "Rishabh Agarwal",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^8.0.0",
    "concat": "^1.0.3",
    "autoprefixer": "^7.1.4",
    "postcss-cli": "^4.1.1"
  }
}
